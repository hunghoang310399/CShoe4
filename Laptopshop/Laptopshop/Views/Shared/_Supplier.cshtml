@model IEnumerable<Laptopshop.Models.Supplier>
<div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
    <ul class="nav navbar-nav">
        <li class="active"><a href="~/Home/Index" class="act">Trang chủ</a></li>
        <!-- Mega Menu -->
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Thương hiệu<b class="caret"></b></a>
            <ul class="dropdown-menu multi-column col_1_of_2">
                <div class="row">
                    <div class="col-sm-12  multi-gd-img">
                        <ul class="multi-column-dropdown">
                            @foreach (var item in Model)
                            {
                                <li><strong><a href="~/Product/ListBySupplier/@item.Id">@item.Name</a></strong></li>
                            }
                        </ul>
                    </div>


                    <div class="clearfix"></div>
                </div>
            </ul>
        </li>


        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Đặt biệt<b class="caret"></b></a>
            <ul class="dropdown-menu multi-column icon-columns">
                <div>
                    <div class="col-sm-10  multi-gd-img">
                        <ul class="multi-column-dropdown">
                            <li><a href="~/Product/ListBySpecial/BEST">Bán chạy nhất</a></li>

                            <li> <a href="~/Product/ListBySpecial/LATEST">Sản Phẩm mới</a></li>

                            <li><a href="~/Product/ListBySpecial/PROMO">Sản phẩm giảm giá</a></li>

                            <li><a href="~/Product/ListBySpecial/SPECIAL">Sản phẩm đặc biệt</a></li>

                            <li><a href="~/Product/ListBySpecial/VIEWS">Sản phẩm được xem nhiều nhất</a></li>

                        </ul>
                    </div>


                    <div class="clearfix"></div>
                </div>
            </ul>
        </li>

    </ul>
</div>

